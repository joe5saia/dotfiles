host server
	HostName remote.joesaia.com
	IdentityFile /home/saiaj/.ssh/server
	Port 15678
	ForwardAgent yes
	ForwardX11 yes

host github.com
	HostName github.com
	user git
	IdentityFile /home/saiaj/.ssh/github
	PreferredAuthentications publickey

